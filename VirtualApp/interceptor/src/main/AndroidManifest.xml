<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="june.legency.env">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>

    <application
        android:allowBackup="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="june.legency.env.SettingActivity"
            android:excludeFromRecents="false"
            android:icon="@mipmap/ev_launcher"
            android:label="@string/ev_app"
            android:launchMode="singleTask"
            android:process=":setting">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="june.legency.env.DetailActivity">
        </activity>
    </application>

</manifest>